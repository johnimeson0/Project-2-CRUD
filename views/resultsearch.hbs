<head>
  <link rel="stylesheet" href="stylesheets/resulSearchStyle.css" />
  <link rel="stylesheet" href="styles/style.css" />
</head>

{{!-- post the result of search --}}
<h1>List</h1>

<div>
{{#each users}}
<section>
    <div>
        <p>{{this.imgUrl}}</p>
    </div>
    <div>
        <h2>{{this.name}}</h2>
        <p>{{this.location}}</p>
        <p>{{this.firstParameter}}</p>
        <p>{{this.secondParameter}}</p>
        <p>{{this.thirdParameter}}</p>
    </div>
    <div>
        <a href="/profile/{{_id}}">View profile</a>
        <form action="/chat/create/{{id}}" method="post">
             <button>Chat with user</button>
        </form>
    </div>
</section>
{{/each}}
 
</div>